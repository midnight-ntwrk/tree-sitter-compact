{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "compact",
      "camelcase": "Compact",
      "title": "Compact",
      "scope": "source.compact",
      "file-types": [
        "compact"
      ],
      "injection-regex": "^compact$",
      "class-name": "TreeSitterCompact"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "Apache",
    "description": "A zero-knowledge smart contract programming language",
    "authors": [
      {
        "name": "Lucas Rosa",
        "email": "lucas.rosa@shielded.io",
        "url": "https://rvcas.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/midnight-ntwrk/tree-sitter-compact"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}